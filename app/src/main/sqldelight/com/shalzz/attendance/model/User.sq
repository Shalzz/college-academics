CREATE TABLE user (
    id TEXT PRIMARY KEY NOT NULL,
    phone TEXT NOT NULL,
    roll_number TEXT,
    name TEXT,
    course TEXT,
    email TEXT
);

selectAll:
SELECT *
FROM user;

selectById:
SELECT *
FROM user
WHERE id = ? ;

selectCount:
SELECT count(*)
FROM user;

insertUser:
INSERT OR REPLACE
INTO user(id, roll_number, name, course, phone, email)
VALUES (?, ?, ?, ?, ?, ?);

deleteAll:
DELETE FROM user;

dropTable:
DROP TABLE IF EXISTS user;