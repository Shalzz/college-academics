CREATE TABLE period (
    id INTEGER AS Integer NOT NULL,
    name TEXT NOT NULL,
    teacher TEXT NOT NULL,
    room TEXT NOT NULL,
    batchid TEXT NOT NULL,
    batch TEXT,
    start TEXT NOT NULL,
    'end' TEXT NOT NULL,
    absent INTEGER AS java.lang.Boolean NOT NULL,
    'date' TEXT AS java.util.Date NOT NUll,
    PRIMARY KEY(id, 'date')
);

select_by_date:
SELECT *
FROM period
WHERE 'date' = ?
ORDER by start;

select_count:
SELECT count(*)
FROM period;